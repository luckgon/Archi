  <folder name="Motivation" id="id-2cae9cd600254943a797cb055e67b574" type="motivation">
    <element xsi:type="archimate:Value" name="Value" id="id-db7c983722cc4439a25c170309a05d10"/>
    <element xsi:type="archimate:Stakeholder" name="Presidente da CD software house(Dilan)" id="id-0ab5fa13a45f4b8ba100e39f82ac1993"/>
    <element xsi:type="archimate:Driver" name="Não pode mais ocorrer atendimento de suporte de maneira informal, pois clientes estão cancelando contratos com a nossa empresa por insastisfação com o atendimento" id="id-61a24da5f2034dbbb83ee9b9156d8009"/>
    <element xsi:type="archimate:Value" name="Aumentará a rentenção de clientes (Renovação de contratos) perdemos em media 120.000 em contratos cancelados por insatisfação com o suporye tecnico" id="id-bb557bf57f444bc2ac4b7a433811fac3"/>
    <element xsi:type="archimate:Value" name="Um cliente satisfeito amplica negocio e indica novos clientes. Ampliamos 280.000 por ano do nosso faturamento com as amplicações de negocios partindo de indicações dos clientes satisfeitos" id="id-91bb99b08178407ba8beabda38dafe89"/>
    <element xsi:type="archimate:Assessment" name="Regitro e acompanhamento dos atendimentos ate o encerramento usando um distema digital(informatizado)" id="id-f4adcfe1380f400cacdde7f205a523d0"/>
    <element xsi:type="archimate:Assessment" name="Ter um meio de acompanhar o grau de satisfação dos clientes com o suporte de maneira formal" id="id-da89fc4317b340ffb7757187899377ba"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-bca42979b4e8459d93c03c13626435e9"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-65c26f2ce0cc4b6bbc402ffb8f39042d"/>
    <element xsi:type="archimate:Outcome" name="informatização da abertura de ocorrencias tecnicas do suporte" id="id-c3d5809602d04d228d5b12746e72aa82"/>
    <element xsi:type="archimate:Goal" name="informatizar o processo de diagnostico e solução de chamados" id="id-749b52347e874eaf8261e5b9f07c7959"/>
    <element xsi:type="archimate:Goal" name="informatização da abertura de ocorrencias tecnicas do suporte" id="id-06d52f431e9b44a581fa7dee15b1436c"/>
    <element xsi:type="archimate:Goal" name="Permitir que o cliente atraves do uso de software possa dar o seu feedback do seu atendimento de forma objetiva" id="id-53265f296b1f4a35b8ff9766077cabd4"/>
    <element xsi:type="archimate:Outcome" name="100% dos chamados precisam estar registrados no sistema novo, sendo que essa solução de documentação digital chamados precisa ser liberada até dia 31/12/2023" id="id-df6333091277479fb1e5f430adb37166"/>
    <element xsi:type="archimate:Outcome" name="O cliente deve pontuar o atendimento com notas objetiva. Esse sistema de avaliação deve estar disponivel 3 meses apos a implantação do sistema de registro de acompanhamento de chamados " id="id-d0ed0a95895f41028abe5b2997a7766e"/>
    <element xsi:type="archimate:Principle" name="Não obrigar o cliente a responder a pesquisa de satisfação. A avaliação é opção do cliente" id="id-b0c930570392450a92625219c5224362"/>
    <element xsi:type="archimate:Principle" name="Ao solucionar um chamado, a pesquisa de satisfaão tem que ser disparada para o cliente" id="id-17bc183745b1434abf0185501a9b687f"/>
    <element xsi:type="archimate:Principle" name="Recepcionar os chamdos direcionando imediatamente para uma equipe especializada no tipo de problema que o cliente enfrenta" id="id-021b6094201440369fbdd30643552363"/>
    <element xsi:type="archimate:Principle" name="Prover uma base de conhecimento sobre problemas e soluções ja catalogados para agilizar a solução" id="id-731b401042f342ee9351db97048511d0"/>
